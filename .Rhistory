xlim(50, 200)
p2 <- ggplot(mtcars, aes(hp, mpg)) +
geom_point(size = 4, alpha = 0.6, colour = "#22333b") +
geom_smooth(size = 1.5, se = F, colour = "#3a86ff") +
md_theme_bw()
p1 + annotation_custom(ggplotGrob(p2), xmin = 125, xmax = 200,
ymin = 22, ymax = 35)
embedded_plot <- ggplot(mtcars, aes(hp, mpg)) +
geom_point(size = 4, alpha = 0.6, colour = "#22333b") +
geom_smooth(size = 1.5, se = F, colour = "#3a86ff") +
md_theme_bw()
p1 + annotation_custom(ggplotGrob(embedded_plot), xmin = 125, xmax = 200,
ymin = 22, ymax = 35)
base_plot + annotation_custom(ggplotGrob(embedded_plot), xmin = 125, xmax = 200,
ymin = 22, ymax = 35)
